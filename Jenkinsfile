pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                echo 'Checkout da branch realizada com sucesso'
            }
        }
        stage('Build') {
            steps {
                echo 'Build simples em execução...'
            }
        }
        stage('Post-build') {
            steps {
                echo 'Build concluído'
            }
        }
    }
}
